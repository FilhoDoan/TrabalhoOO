package ClassesUml;

public class Vendedor extends Cliente {
	private long valNegociacao;
	private Veiculo veiculo;
	
	
	public Vendedor(String nome, String email, int telefone, long cpf) {
		super(nome, email, telefone, cpf);
		// TODO Auto-generated constructor stub
	}
	
	public Vendedor(String nome, String email, int telefone, long cpf, long valNegociacao, Veiculo veiculo) {
		super(nome, email, telefone, cpf);
		this.valNegociacao = valNegociacao;
		this.veiculo = veiculo;
	}

	public long getValNegociacao() {
		return valNegociacao;
	}

	public void setValNegociacao(long valNegociacao) {
		this.valNegociacao = valNegociacao;
	}

	public Veiculo getVeiculo() {
		return veiculo;
	}

	public void setVeiculo(Veiculo veiculo) {
		this.veiculo = veiculo;
	}
	
	public void cadastrarVeiculo() {
		
	}
	
	
	

	
}


